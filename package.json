{
  "name": "@aikotools/datafilter",
  "version": "0.0.0-semantic-release",
  "description": "Advanced data filtering engine for JSON file matching in E2E testing",
  "type": "module",
  "main": "dist/aikotools-datafilter.cjs",
  "module": "dist/aikotools-datafilter.mjs",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/aikotools-datafilter.mjs"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/aikotools-datafilter.cjs"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"{src,tests}/**/*.{js,ts,json}\"",
    "lint": "eslint \"{src,tests}/**/*.{js,ts}\"",
    "depcheck": "depcheck",
    "build": "npm run format && npm run lint && npm run depcheck && tsc && vite build",
    "test": "npm run build && vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "docs:build": "bash scripts/build-readme.sh"
  },
  "keywords": [
    "filter",
    "json",
    "e2e",
    "testing",
    "matcher",
    "data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "depcheck": "^1.4.7",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.3"
  }
}
